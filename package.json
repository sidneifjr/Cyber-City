  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "cssnano": "^4.1.10",
    "parcel": "latest",
    "pug": "^2.0.4",
    "sass": "^1.26.3",
    "typescript": "^3.9.5",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/node": "^14.0.24",
    "components": "^0.1.0",
    "font-awesome": "^4.7.0",
    "hamburgers": "^1.1.3",
    "is-expression": "^3.0.0",
    "parcel-plugin-custom-dist-structure": "^1.1.14",
    "uglify-js": "^3.8.0",
    "with": "^6.0.0"
  },
  "scripts": {
    "dev": "parcel frontend/views/index.pug --port 9000 --hmr-port 1235 --out-dir .dist --cache-dir frontend/.cache",
    "devopen": "parcel frontend/views/index.pug --open 'google chrome'",
    "build": "parcel build frontend/views/index.pug --no-content-hash --out-dir build --public-url ./"
  },
  "customDistStructure": {
    "config": {
      ".css": "css",
      ".js": "js",
      "images": [
        ".jpg",
        ".png"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    },
    "options": {
      "development": false
    }
  }
}
